  <SQLQuery>
    <![CDATA[SELECT lookup_code, meaning, view_application_id, lookup_type 
FROM fnd_lookup_values 
WHERE trunc(nvl(start_date_active, sysdate)) <= trunc(sysdate) AND trunc(nvl(end_date_active, sysdate)) >= trunc(sysdate) 
AND enabled_flag = 'Y' 
AND language = USERENV('LANG') 
AND lookup_type = 'SALES_CHANNEL'
order by meaning]]>
  </SQLQuery>
