<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="IncoTermVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|VarAccess"/>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="xxatcust.oracle.apps.sudoku.model.readonlyvo.IncoTermVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT 
  lookup_code,
  meaning,
  view_application_id,
  lookup_type,
  tag
FROM 
  fnd_lookup_values
WHERE 
  trunc(nvl(start_date_active, sysdate)) <= trunc(sysdate) AND
  trunc(nvl(end_date_active, sysdate)) >= trunc(sysdate) AND
  enabled_flag = 'Y'  AND
  language = USERENV('LANG') AND
  lookup_type = 'FOB'
 AND tag='Y']]>
  </SQLQuery>
  <ViewAttribute
    Name="LookupCode"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="30"
