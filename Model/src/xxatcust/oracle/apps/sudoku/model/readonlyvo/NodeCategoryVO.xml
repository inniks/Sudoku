  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="InventoryItemId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="xxatcust.oracle.apps.sudoku.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
