<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="NodeCategoryVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="xxatcust.oracle.apps.sudoku.model.readonlyvo.NodeCategoryVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[select inventory_item_id, segment1, attribute7
from mtl_system_items_b
where inventory_item_id in (
select component_item_id
from bom_explosions
where top_item_id = 469330
)
and organization_id = 146
and customer_order_enabled_flag = 'Y'
order by segment1]]>
  </SQLQuery>
  <ViewAttribute
    Name="InventoryItemId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="BIGINT"
    AliasName="INVENTORY_ITEM_ID"
    Expression="INVENTORY_ITEM_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Segment1"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SEGMENT1"
    Expression="SEGMENT1"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Attribute7"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ATTRIBUTE7"
    Expression="ATTRIBUTE7"
